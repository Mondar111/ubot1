_starter () {
    echo "Downloading source code"
    git clone https://github.com/AyiinXd/AyinUbot
    cd AyinUbot/ && rm -rf .git
    python3 ../updater.py ../requirements.txt ../requirements.txt
    echo "AyiinXd version - 0.1.0, Copyright Â© 2022 (Ayiin) <https://github.com/AyiinXd>"
    python3 -m AyiinXd
}

_starter